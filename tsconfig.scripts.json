{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "outDir": "dist-scripts",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["scripts/**/*"]
}
